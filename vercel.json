{
    "version": 2,
    "builds": [
        {
            "src": "./app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/app/(.*)",
            "dest": "/app/wsgi.py"
        }
    ]
}
