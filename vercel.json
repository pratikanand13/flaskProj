{
    "version": 2,
    "builds": [
      {
        "src": "./wsgi.py", 
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.11"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "buildCommand": "brew install postgresql && pip install -r requirements.txt"
  }
  